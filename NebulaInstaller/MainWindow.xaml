<Window x:Class="NebulaInstaller.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NebulaInstaller"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Nebula on the Ship Setup" Height="380" Width="620">
    <Grid>
        <Rectangle Margin="0,60,0,0" Fill="#FAFDFF"/>
        <Frame x:Name="InnerFrame" NavigationUIVisibility="Hidden" Margin="0,60,0,0"/>
        <Image Margin="0,0,0,0" Height="60" VerticalAlignment="Top" HorizontalAlignment="Left" Width="620" Source="Resources/BackImage.png"/>
    </Grid>
</Window>
